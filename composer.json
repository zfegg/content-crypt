{
  "name": "zfegg/http-content-crypt",
  "description": "HTTP content crypt/signature for PSR7 middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "Moln",
      "email": "moln.xie@gmail.com"
    }
  ],
  "keywords": [
    "middleware",
    "content",
    "signature",
    "crypt",
    "psr7",
    "zfegg"
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "zendframework/zend-crypt": "^3.2",
    "zendframework/zend-diactoros": "^1.3"
  },
  "autoload":{
    "psr-4":{
      "Zfegg\\HttpContentCrypt\\":"src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfeggTest\\HttpContentCrypt\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5",
    "squizlabs/php_codesniffer": "^2.7"
  }
}
